Proceso ejercicio4
Definir num1,num2 Como Real;
Escribir "Añada el valor de los dos números";
Leer num1,num2;
Si num1=num2 Entonces
Escribir "El valor final sera = ",num1*num2;
SiNo
	Si num1>num2 Entonces
		Escribir "El valor final sera = ",num1-num2;
	SiNo
		Escribir "El valor final sera = ",num1+num2;
	FinSi
FinSi
FinProceso
